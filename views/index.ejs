<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <title></title>
</head>

<body>
    <nav>
        <a href="/products/new">Add a New Item</a>
    </nav>
    <h1>Products Index</h1>
    <ul>
        <%console.log(products) %>
        <% products.forEach(product=> { %>
                <h2><%= product.name %> </h2>
                <a href="/products/<%= product._id %>"><img src="<%= product.img%>"></a>
                <p><%= product.price%></p>
                <p><%= product.qty%></p>
        <% }) %> 
    </ul>
</body>
</html>